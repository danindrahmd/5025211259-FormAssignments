# Ignore directories and files generated by Composer and NPM
/vendor/
/node_modules/

# Ignore Laravel Mix and compiled assets
/public/css/
/public/js/
/public/mix-*.js
/public/mix-*.css

# Ignore Laravel storage and framework directories
/storage/*.key
/storage/debugbar
/storage/app/public
/storage/app/tmp
/storage/framework/*
/storage/logs/*

# Ignore Laravel cache directories
/bootstrap/cache/
/node_modules_cache/
/debugbar/*

# Ignore PHPUnit testing framework files
/phpunit.xml
/tests/

# Ignore PHPStorm, VS Code, and Sublime Text project files
.idea/
.vscode/
*.sublime-workspace

# Ignore environment configuration files
.env

# Ignore Laravel Homestead and Vagrant files
Homestead.yaml
Homestead.json
Vagrantfile

# Ignore database files
/database/*.sqlite
/database/*.sql

# Ignore Laravel Passport, Spark, and Dusk files
/storage/oauth-private.key
/storage/oauth-public.key
/storage/screenshots/
/storage/app/dusk.sqlite
spark-*.key
.env.dusk.local
.env.dusk.testing
.env.dusk.staging
.env.dusk.production

# Ignore user-specific files and OS-generated files
*.iml
*.log
*.tmd
.DS_Store
Thumbs.db
